{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "node": true,
    "jest": true,
    "jquery": true
  },
  "rules": {
    "quotes": [1, "double"],
    "no-unused-vars": [
      "warn",
      {
        "ignoreRestSiblings": true
      }
    ],
    "no-console": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017
  },

}
